    <string name="fingerprint_unlock_set_unlock_password">Pixel Imprint + પાસવર્ડ</string>

    <!-- Title for preference that guides the user to skip fingerprint setup [CHAR LIMIT=60]-->
    <string name="fingerprint_unlock_skip_fingerprint">Pixel Imprint વગર આગળ વધો</string>

