    <!-- Message shown in screen lock picker while setting up the new screen lock with fingerprint option. [CHAR LIMIT=NONE]-->
    <string name="fingerprint_unlock_title">Barmaq izindən istifadə edərək Pixel Imprint ilə telefonu kiliddən çıxara bilərsiniz. Təhlükəsizlik üçün bu seçim yedək ekran kilidi tələb edir.</string>

    <!-- Summary for suggested actions for settings up a fingerprint lock (phone) [CHAR LIMIT=55] -->
    <string name="suggested_fingerprint_lock_settings_summary">Telefon kilidini açmaq üçün Pixel Imprint istifadə edin</string>

