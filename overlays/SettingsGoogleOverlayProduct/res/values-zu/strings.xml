    <!-- Message shown in screen lock picker while setting up the new screen lock with fingerprint option. [CHAR LIMIT=NONE]-->
    <string name="fingerprint_unlock_title">Nge-Pixel Imprint, ungavula ifoni yakho usebenzisa izigxivizo zeminwe yakho. Ngokuphepha, le nketho idinga ukukhiya kwesikrini okuyisipele.</string>

    <!-- Summary for suggested actions for settings up a fingerprint lock (phone) [CHAR LIMIT=55] -->
    <string name="suggested_fingerprint_lock_settings_summary">Sebenzisa i-Pixel Imprint ukuvula ifoni yakho</string>

